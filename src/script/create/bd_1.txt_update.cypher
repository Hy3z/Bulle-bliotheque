MATCH (b:Book {ISBN_13:"9782205025712"}) SET b.ISBN_10 = "2205025716", b.description = "Bandes dessinées. Curieuse histoire qui commence en western, se poursuit en comédie mondaine pour faire place à une chronique de guerre.", b.publishedDate = "1984", b.pageCount = 48
