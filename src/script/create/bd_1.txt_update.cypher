MATCH (b:Book {ISBN_13:"9782731610451"}) SET b.ISBN_10 = "273161045X", b.description = "\"Comme \"Plagiat !\", cette nouvelle histoire est inspirée d'un fait divers réel, l'affaire Enigma. Un jeune journaliste part à la recherche d'un scientifique étrange et de son secret perdu dans les méandres de la Seconde Guerre mondiale.", b.publishedDate = "1992", b.pageCount = 46
