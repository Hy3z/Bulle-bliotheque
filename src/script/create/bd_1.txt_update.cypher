MATCH (b:Book {ISBN_13:"9782302005587"}) SET b.ISBN_10 = "2302005589", b.description = "Quand les dragons apparaissent, le chaos les suit et ravage les régions alentour. Un chaos que seules les jeunes vierges peuvent traverser indemnes...", b.publisher = "Soleil Productions", b.publishedDate = "2009", b.pageCount = 46
