MATCH (b:Book {ISBN_13:"9782840550150"}) SET b.ISBN_10 = "2840550156", b.description = "\"Dès leur arrivée au 30e siècle, l'agent secret Campbell et la scientifique Tess Wood sont séparés par une gigantesque explosion. Chacun de son côté et dans sa propre série, ces deux héros vont essayer de se retrouver et de mener à bien leur mission!", b.publishedDate = "1993-10-01", b.pageCount = 47
