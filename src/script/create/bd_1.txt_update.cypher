MATCH (b:Book {ISBN_13:"9782723415286"}) SET b.ISBN_10 = "2723415287", b.description = "\" Oté, Névé ! Tu fais un drôle de marmaille. Tu ne souris pas souvent... Mais, après tout, je ne connais rien de toi... Parle-moi un peu de tes parents... \"", b.publishedDate = "1992-10-01", b.pageCount = 48
