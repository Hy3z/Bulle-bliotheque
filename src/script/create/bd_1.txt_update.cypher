MATCH (b:Book {ISBN_13:"9782871290728"}) SET b.ISBN_10 = "2871290725", b.description = "\"XIII a été une fois de plus manipulé, tout d'abord par Minecro, une puissante et mystérieuse compagnie dont le plan était de le faire passer pour el Cascador, héros de la révolution costaverdienne.", b.publisher = "Dargaud", b.publishedDate = "1994", b.pageCount = 48
