CREATE (:Book {ISBN_13: "9782800131733", title: "Chinaman T.3: Pour Rose", cote: "CHINA", ISBN_10: "280013173X", description: "Exilé dans les Rocheuses, Chinaman croise le chemin de Rose, une jeune orpheline. Injustement accusé d'avoir kidnappé l'enfant, il est à nouveau traqué et doit reprendre les armes.", publisher: "Editions Dupuis", publishedDate: "2001", pageCount: 47})
